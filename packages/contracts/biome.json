{
  "$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
  "extends": ["../../biome.json"],
  "files": {
    "ignore": [
      "cache",
      "cache_forge",
      "coverage",
      "coverage.json",
      "artifacts",
      "broadcast",
